{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/src/**/*.ts",
    "**/src/**/*.tsx",
    "!**/node_modules/**"
  ],
  "coverageReporters": [
    "json",
    "lcov",
    "text",
    "clover",
    "text-summary"
  ],
  "modulePathIgnorePatterns": ["dist"],
  "testEnvironment": "jest-environment-jsdom",
  "testMatch": [
    "**/__tests__/**/*.ts",
    "**/__tests__/**/*.tsx"
  ]
}

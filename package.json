{
  "name": "tts-react",
  "version": "0.2.1",
  "description": "React component to convert text to speech.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/morganney/tts-react.git"
  },
  "bugs": {
    "url": "https://github.com/morganney/tts-react/issues"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions '.ts,.tsx'",
    "build": "npm run build:types && npm run build:js",
    "lint": "eslint . src demo --ignore-pattern dist --ext .ts,.tsx",
    "prettier": "prettier --no-error-on-unmatched-pattern --write *.ts {src,demo}/**/*.{ts,tsx}",
    "demo": "npm run build && start-storybook -c demo/.storybook -p 3000",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepack": "npm run build"
  },
  "engines": {
    "npm": ">=8.3.0"
  },
  "keywords": [
    "text-to-speech",
    "tts",
    "react",
    "speech",
    "synthesis",
    "amazon-polly"
  ],
  "author": "Morgan Ney <morganney@gmail.com>",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^17.0.0 || ^18.2.0",
    "react-dom": "^17.0.0 || ^18.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@faker-js/faker": "^7.4.0",
    "@storybook/addon-a11y": "^6.5.10",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-controls": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "circular-dependency-plugin": "^5.2.2",
    "codecov": "^3.8.3",
    "eslint": "^8.23.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.0.1",
    "prettier": "^2.7.1",
    "typescript": "4.7.4"
  }
}

{
  "name": "tts-react-monorepo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.18.1",
    "npm": ">=10.8.2"
  },
  "engineStrict": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "precheck-types": "npm run build:tts",
    "check-types": "npm run check-types --workspaces",
    "build:tts": "npm run build -w tts-react",
    "local:build": "npm run build:tts && http-server -o build.html",
    "prebuild:story": "npm run build:tts",
    "build:story": "npm run build -w story",
    "prestory": "npm run build:tts",
    "story": "npm run start -w story",
    "lint": "eslint packages/**/{src,__tests__}/**/*.{ts,tsx}",
    "lint:fix": "eslint --fix packages/**/{src,__tests__}/**/*.{ts,tsx}",
    "prettier": "prettier --no-error-on-unmatched-pattern --write *.ts packages/**/{src,__tests__}/**/*.{ts,tsx}"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@eslint/js": "^9.32.0",
    "@knighted/duel": "^2.1.5",
    "@testing-library/dom": "^10.3.0",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "babel-jest": "^30.0.0-alpha.7",
    "eslint": "^9.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "http-server": "^14.1.1",
    "jest": "^30.0.0-alpha.7",
    "jest-environment-jsdom": "^30.0.0-alpha.7",
    "prettier": "^3.2.5",
    "ts-jest-resolver": "^2.0.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0"
  },
  "overrides": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tough-cookie": "^5.0.0",
    "whatwg-url": "^14.0.0",
    "jsdom": "^26.0.0",
    "rimraf": "^4.4.1",
    "test-exclude": {
      "glob": "^9.3.5"
    }
  }
}
